 <h1>History list</h1> 
<div class="container">
  <div class="row">
    <% @texts.each do |text| %>
    <div class="one-tweet col-md-4 col-sm-10 col-ofset-2">
     <div class= "tweet-box">
      <p><%= text.content %></p>
      <p><%= text.created_at.strftime("%Y年%m月%d日 %H時%M分") %></p>
     <%= link_to "Edit",edit_text_path(text.id),class:"btn btn-success btn-border" %>
     <%= link_to "Delete",text_path(text.id),method: :delete,class:"btn btn-danger btn-border", data:{confirm:"本当に削除しますか？"} %>
     </div>
   </div>
   <% end %>
  </div>
</div>
